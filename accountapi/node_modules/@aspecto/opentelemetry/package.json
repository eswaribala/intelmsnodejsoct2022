    },
    "scripts": {
        "test": "echo \"Error: run tests from root\" && exit 1",
        "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist/src && rimraf dist/src/index.d.ts.map",
        "build": "tsc && yarn copy-dts",
        "prebuild": "rm -rf ./dist",
        "prepare": "yarn run build",
        "watch": "tsc-watch --onSuccess \"yarn copy-dts\"",
        "preuninstall": "npx aspecto-otel-live uninstall || exit 0",
        "prettier": "prettier --config .prettierrc --write \"src/**/*.{ts,js,json}\"",
        "version:update": "node ../../scripts/version-update.js",
        "version": "yarn run version:update"
    },
    "publishConfig": {
        "access": "public"
    },
